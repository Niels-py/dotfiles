profile main {
    output HDMI-A-1 {
        enable
        position 0,0
        scale 0.75
    }

    output DP-1 {
        enable 
        position 2560,0
        adaptive_sync off
    }
}

profile old {
    output HDMI-A-1 {
        enable
        position 0,0
        transform 90
        scale 0.75
    }

    output DP-1 {
        enable 
        position 1440,0
        adaptive_sync off
    }
}

profile zzz {
    output HDMI-A-1 disable
    output DP-1 disable
}
